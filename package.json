{
  "name": "ngrx-slice",
  "version": "3.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build && npm run scully -- --configuration production --scanRoutes",
    "build:lib": "ng build ngrx-slice --configuration=production",
    "copy-readme": "shx cp README.md projects/ngrx-slice",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "release": "dotenv release-it --",
    "publish": "cd dist/ngrx-slice && npm publish",
    "scully": "npx scully -- --project docs",
    "scully:serve": "npx scully serve -- --project docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.2",
    "@angular/cdk": "^12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@ngrx/effects": "^12.4.0",
    "@ngrx/store": "^12.4.0",
    "@scullyio/init": "^1.1.4",
    "@scullyio/ng-lib": "^1.1.1",
    "@scullyio/scully": "^1.1.1",
    "@taiga-ui/addon-doc": "^2.14.0",
    "@taiga-ui/addon-mobile": "^2.14.0",
    "@taiga-ui/cdk": "^2.14.0",
    "@taiga-ui/core": "^2.14.0",
    "@taiga-ui/icons": "^2.14.0",
    "@taiga-ui/kit": "^2.14.0",
    "immer": "^9.0.5",
    "ngrx-immer": "1.0.1",
    "ngx-highlightjs": "^4.1.4",
    "rxjs": "~7.3.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@release-it/bumper": "^3.0.1",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jasmine": "~3.8.2",
    "@types/node": "^12.11.1",
    "dotenv-cli": "^4.0.0",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.1",
    "prettier": "2.3.2",
    "raw-loader": "^4.0.2",
    "release-it": "^14.11.5",
    "shx": "^0.3.3",
    "typescript": "~4.3.5"
  },
  "prettier": {
    "semi": true,
    "htmlWhitespaceSensitivity": "ignore"
  }
}
