{
  "name": "ngrx-slice",
  "version": "5.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run build && npm run scully -- --configuration production --scanRoutes",
    "build:lib": "ng build ngrx-slice --configuration=production",
    "copy-readme": "shx cp README.md projects/ngrx-slice",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "release": "dotenv release-it --",
    "publish": "cd dist/ngrx-slice && npm publish",
    "scully": "npx scully -- --project docs",
    "scully:serve": "npx scully serve -- --project docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.11",
    "@angular/cdk": "^12.2.11",
    "@angular/common": "~12.2.11",
    "@angular/compiler": "~12.2.11",
    "@angular/core": "~12.2.11",
    "@angular/forms": "~12.2.11",
    "@angular/platform-browser": "~12.2.11",
    "@angular/platform-browser-dynamic": "~12.2.11",
    "@angular/router": "~12.2.11",
    "@ngrx/effects": "^12.5.0",
    "@ngrx/store": "^12.5.0",
    "@scullyio/init": "^2.0.1",
    "@scullyio/ng-lib": "^2.0.0",
    "@scullyio/scully": "^2.0.1",
    "@taiga-ui/addon-doc": "^2.20.0",
    "@taiga-ui/addon-mobile": "^2.20.0",
    "@taiga-ui/cdk": "^2.20.0",
    "@taiga-ui/core": "^2.20.0",
    "@taiga-ui/icons": "^2.20.0",
    "@taiga-ui/kit": "^2.20.0",
    "immer": "^9.0.6",
    "ngrx-immer": "1.0.1",
    "ngx-highlightjs": "^5.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@release-it/bumper": "^3.0.1",
    "@release-it/conventional-changelog": "^3.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^16.11.4",
    "dotenv-cli": "^4.0.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^12.2.4",
    "prettier": "2.4.1",
    "raw-loader": "^4.0.2",
    "release-it": "^14.11.6",
    "shx": "^0.3.3",
    "typescript": "~4.3.5"
  },
  "prettier": {
    "semi": true,
    "htmlWhitespaceSensitivity": "ignore"
  }
}
